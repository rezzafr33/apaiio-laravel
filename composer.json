{
    "name": "rezzafr33/apaiio-laravel",
    "homepage": "https://github.com/rezzafr33/apaiio-laravel",
    "description": "A simple Laravel 5 service provider for including the ApaiIO / Amazon Product Advertising PHP Library.",
    "type":"library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Apaiio\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Apaiio\\Laravel\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~5.3",
        "exeu/apai-io": "~2.0"
    },
    "require-dev": {
        "laravel/framework": "~5.3",
        "phpunit/phpunit": "4.*"
    }
}
